artApp() {\r\n            // Kiwi IRC version this is built from\r\n            kiwi.build_version = '0.9.4';\r\n\r\n            // Start the app after loading plugins\r\n            kiwi.init(opts, function() {\r\n                if (opts.client_plugins \u0026\u0026 opts.client_plugins.length \u003e 0) {\r\n\r\n                    // Wait until all plugins are loaded before starting the app\r\n                    kiwi.plugins.once('loaded', function() {\r\n                        kiwi.start();\r\n                    });\r\n\r\n                    _.each(opts.client_plugins, function (plugin_url) {\r\n                        kiwi.plugins.load(plugin_url);\r\n                    });\r\n\r\n                } else {\r\n\r\n                    // No plugins were needed so start the app\r\n                    kiwi.start();\r\n                }\r\n            });\r\n        });\r\n    })();\r\n\u003c/script\u003e\r\n\u003c/body\u003e\r\n\u003c/html\u003e\r\n","pageTitle":" Jungs Chat ","responseHeaders":{"CACHE-CONTROL":"max-age=3600","CONNECTION":"keep-alive","CONTENT-TYPE":"text/html","DATE":"Sun, 24 Jul 2016 00:10:40 GMT","ETAG":"W/\"3456-29462-1466355397000\"","LAST-MODIFIED":"Sun, 19 Jun 2016 16:56:37 GMT","SERVER":"nginx","VARY":"Accept-Encoding"},"bitcoinAddresses":null,"sshKey":"","ftpFingerprint":"","ftpBanner":"","smtpFingerprint":"","smtpBanner":""}
