003c/head\u003e\n  \u003cbody\u003e\n    \u003cdiv id=\"wrap\"\u003e\n      \u003cdiv class=\"container\"\u003e\n        \u003cdiv class=\"page-header\" id=\"banner\" style=\"border-bottom: 0px;\"\u003e\n          \u003cdiv class=\"row\"\u003e\n            \u003cdiv class=\"col-lg-12\"\u003e\n              \u003ch1 class=\"text-center\"\u003e\u003cstrong\u003eOpenPGP\u003c/strong\u003ekeyserver\u003c/h1\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\u003c!--closing page header container--\u003e\n      \u003cdiv class=\"container\"\u003e\n        \u003cdiv class=\"row\"\u003e\n          \u003cdiv class=\"col-lg-8 col-lg-offset-2\"\u003e\n            \u003cform action=\"/pks/lookup\" method=\"get\" class=\"form\" role=\"form\"\u003e\n              \u003cdiv class=\"control-group\"\u003e\n                \u003cdiv class=\"controls\" style=\"max-width:30em; margin:0 auto;\"\u003e\n                  \u003cinput name=\"search\" class=\"form-control\" type=\"text\" placeholder=\"Search in over 4 million OpenPGP keys...\" \n                    style=\"font-size: 1.5em; line-height: 1.5em; height: 2em;\" required /\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"control-group text-center\" style=\"margin-top: 0.5em;\"\u003e\n                \u003cdiv class=\"controls\"\u003e\n                  \u003cbutton type=\"submit\" class=\"btn btn-primary\"\u003e\n                    \u003cspan class=\"glyphicon glyphicon-search\"\u003e\u003c/span\u003e\n                    Search Key\n                  \u003c/button\u003e\n                  \u003cbutton type=\"button\" class=\"btn btn-primary\" data-toggle=\"modal\" data-target=\"#myModal\"\u003e\n                    \u003cspan class=\"glyphicon glyphicon-cloud-upload\"\u003e\u003c/span\u003e\n                    Submit Key\n                  \u003c/button\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"text-center\" style=\"margin-top: 0.5em;\"\u003e\n                \u003ca data-toggle=\"collapse\" data-target=\"#advancedOptionsCollapse\" href=\"#\"\u003eAdvanced Options\u003c/a\u003e\n              \u003c/div\u003e\n              \u003cdiv id=\"advancedOptionsCollapse\" class=\"collapse\"\u003e\n                \u003cdiv class=\"control-group col-md-4 col-md-offset-2\"\u003e\n                  \u003cdiv class=\"controls\"\u003e\n                    Index Options:\n                    \u003clabel class=\"checkbox\" for=\"fingerprint\"\u003e\n                      \u003cinput id=\"fingerprint\" type=\"checkbox\" name=\"fingerprint\" checked=\"checked\" /\u003e\n                      Show OpenPGP fingerprints\n                    \u003c/label\u003e\n                    \u003clabel class=\"checkbox\" for=\"hash\"\u003e\n                      \u003cinput id=\"hash\" type=\"checkbox\" name=\"hash\" /\u003e\n                      Show full-key hashes\n                    \u003c/label\u003e\n                    \u003clabel class=\"checkbox\" for=\"exact\"\u003e\n                      \u003cinput id=\"exact\" type=\"checkbox\" name=\"exact\" value=\"on\" /\u003e\n                      Only return exact matches\n                    \u003c/label\u003e\n                    \u003clabel class=\"checkbox\" for=\"mr\"\u003e\n                      \u003cinput id=\"mr\" type=\"checkbox\" name=\"options\" value=\"mr\" /\u003e\n                      Machine readable\n                    \u003c/label\u003e\n                  \u003c/div\u003e\n                \u003c/div\u003e\n                \u003cdiv class=\"control-group col-md-offset-6\"\u003e\n                  \u003cdiv class=\"controls\"\u003e\n                    Index type:\n                    \u003clabel class=\"radio\" for=\"radios-0\"\u003e\n                      \u003cinput name=\"op\" id=\"radios-0\" value=\"index\" checked=\"checked\" type=\"radio\"\u003e\n                      Get regular index of matching keys\n                    \u003c/label\u003e\n                    \u003clabel class=\"radio\" for=\"radios-1\"\u003e\n                      \u003cinput name=\"op\" id=\"radios-1\" value=\"vindex\" type=\"radio\"\u003e\n                      Get \u003cstrong\u003everbose\u003c/strong\u003e index of matching keys\n                    \u003c/label\u003e\n                    \u003clabel class=\"radio\" for=\"radios-2\"\u003e\n                      \u003cinput name=\"op\" id=\"radios-2\" value=\"get\" type=\"radio\"\u003e\n                      retrieve ascii-armored keys\n                    \u003c/label\u003e\n                    \u003clabel class=\"radio\" for=\"radios-3\"\u003e\n                      \u003cinput name=\"op\" id=\"radios-3\" value=\"hget\" type=\"radio\"\u003e\n                      retrieve keys by full-key hash\n                    \u003c/label\u003e\n                  \u003c/div\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/form\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"modal fade\" id=\"myModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"myModalLabel\" aria-hidden=\"true\"\u003e\n        \u003cdiv class=\"modal-dialog\"\u003e\n          \u003cdiv class=\"modal-content\"\u003e\n            \u003cdiv class=\"modal-header\"\u003e\n              \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e\u0026times;\u003c/button\u003e\n              \u003ch4 class=\"modal-title\" id=\"myModalLabel\"\u003eSubmit Your OpenPGP Public Key\u003c/h4\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"modal-body\"\u003e\n              \u003cform action=\"/pks/add\" method=\"post\" id=\"keySubmitForm\"\u003e\n                \u003cdiv class=\"control-group\"\u003e\n                  \u003cdiv class=\"controls\"\u003e\n                    \u003ctextarea id=\"textarea\" name=\"keytext\" rows=\"20\" \n                      style=\"width: 100%;font-family:Courier;font-size:12px;\" \n                      placeholder=\"Enter your ASCII-armored OpenPGP public key here\" required \u003e\n                    \u003c/textarea\u003e\n                  \u003c/div\u003e\n                \u003c/div\u003e\n                \u003cdiv class=\"modal-footer\"\u003e\n                  \u003cbutton type=\"button\" class=\"btn btn-default\" data-dismiss=\"modal\"\u003eClose\u003c/button\u003e\n                  \u003cbutton type=\"submit\" class=\"btn btn-primary\"\u003e\n                    \u003cspan class=\"glyphicon glyphicon-cloud-upload\"\u003e\u003c/span\u003e\n                    Submit Public Key\n                  \u003c/button\u003e\n                \u003c/div\u003e\n              \u003c/form\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"modal fade\" id=\"contact\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"ContactLabel\" aria-hidden=\"true\"\u003e\n        \u003cdiv class=\"modal-dialog\"\u003e\n          \u003cdiv class=\"modal-content\"\u003e\n            \u003cdiv class=\"modal-header\"\u003e\n              \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e\u0026times;\u003c/button\u003e\n              \u003ch4 class=\"modal-title\" id=\"ContactLabel\"\u003eKeyserver Admin OpenPGP Public Key\u003c/h4\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"modal-body\"\u003e\n              \u003cp\u003e\n                If you have any issues or concerns about this site, or if you \n                wish to peer with this server, please contact me via the email \n                address within the below public key.\n              \u003c/p\u003e\n              \u003cpre\u003e\n0x27a69fc9a1744242\n              \u003c/pre\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n\n      \u003cdiv class=\"modal fade\" id=\"donate\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"DonateLabel\" aria-hidden=\"true\"\u003e\n        \u003cdiv class=\"modal-dialog\"\u003e\n          \u003cdiv class=\"modal-content\"\u003e\n            \u003cdiv class=\"modal-header\"\u003e\n              \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e\u0026times;\u003c/button\u003e\n              \u003ch4 class=\"modal-title\" id=\"DonateLabel\"\u003eDonations too\u003c/h4\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"modal-body\"\u003e\n                \u003cp\u003e\n                  Donations towards the operation of this service can be made to the following bitcoin address:\n                \u003c/p\u003e\n              \u003cdiv class=\"span3\" align=\"center\"\u003e\n                \u003ca href=\"bitcoin:1JUaf3j3SrGGzMAGijBKvUwMC6YvJq2xwX?message=keyserver donation\"\u003e\n                  \u003cimg src=\"assets/img/qr.png\" width=150px height=150px /\u003e\n                \u003c/a\u003e\n              \u003c/div\u003e\n              \u003cpre\u003ebitcoin:1JUaf3j3SrGGzMAGijBKvUwMC6YvJq2xwX\u003c/pre\u003e\n              \u003cp\u003e\n                Thank you.\n              \u003c/p\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n\n      \u003cdiv class=\"modal fade\" id=\"about\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"AboutLabel\" aria-hidden=\"true\"\u003e\n        \u003cdiv class=\"modal-dialog\"\u003e\n          \u003cdiv class=\"modal-content\"\u003e\n            \u003cdiv class=\"modal-header\"\u003e\n              \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e\u0026times;\u003c/button\u003e\n              \u003ch4 class=\"modal-title\" id=\"AboutLabel\"\u003eAbout pgpkeys.urown.net\u003c/h4\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"modal-body\"\u003e\n              \u003cp\u003e\n               With \u003cstrong\u003e\u003ca href=\"https://emailselfdefense.fsf.org/en/\"\n               title=\"Email Self-Defense\" target=\"_blank\" \u003eOpenPGP\u003c/a\u003e\u003c/strong\u003e\n               you can encrypt and sign mail and other data \u003cem\u003eend to\n               end\u003c/em\u003e, using \u003cem\u003epublic key cryptography\u003c/em\u003e. As the name\n               implies, the public keys need to be distributed publicly. That is\n               what keyservers do. Clients like Enigmail for Thunderbird connect\n               to keyservers to search and retrieve the keys they need.\n              \u003c/p\u003e\n              \u003chr /\u003e\n              \u003ch5\u003eTor\u003c/h5\u003e\n                \u003cp\u003epgpkeys.urown.net is accesible as \u003ca href=\"https://www.torproject.org/docs/faq.html.en#TorHiddenServices\"\u003eTor hidden service\u003c/a\u003e at the .onion address \u003cstrong\u003e\u003ca href=\"http://pgpkeysximvxiazm.onion\"\u003epgpkeysximvxiazm.onion\u003c/a\u003e\u003c/strong\u003e.\n              \u003c/p\u003e\n              \u003chr /\u003e\n              \u003ch5\u003eSoftware and Protocols\u003c/h5\u003e\n              \u003cp\u003e\n                pgpkeys.urown.net runs \u003ca href=\"https://bitbucket.org/skskeyserver/sks-keyserver\" target=\"_blank\"\u003eSKS (The Synchronizing OpenPGP Key Server)\u003c/a\u003e and understands the following protocols:\n              \u003c/p\u003e\n                \u003cul\u003e\n                  \u003cli\u003eHTTP and HKP on TCP port 80\u003c/li\u003e\n                  \u003cli\u003eHTTPS and HKPS on TCP port 443 (\u003ca href=\"https://www.ssllabs.com/ssltest/analyze.html?d=pgpkeys.urown.net\" target=\"_blank\"\u003eSSL test\u003c/a\u003e)\u003c/li\u003e\n                  \u003cli\u003eHTTP and HKP on TCP port 11371\u003c/li\u003e\n                \u003c/ul\u003e\n              \u003chr /\u003e\n              \u003ch5\u003eKeyserver Pools\u003c/h5\u003e\n              \u003cp\u003epgpkeys.urown.net is a member of the \u003ca href=\"https://sks-keyservers.net\" target=\"_blank\"\u003esks-keyserver pool\u003c/a\u003e.\u003c/p\u003e\n              \u003cdiv class=\"col-md-6\"\u003e\n              \u003cp\u003e\n                Connect your OpenPGP client to the pool by using one of the following servers:\n              \u003c/p\u003e\n                \u003cul\u003e\n                  \u003cli\u003epool.sks-keyservers.net\u003c/li\u003e\n                  \u003cli\u003ekeys.gnupg.net\u003c/li\u003e\n                  \u003cli\u003epgp.ipfire.org\u003c/li\u003e\n                \u003c/ul\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"col-md-offset-6\"\u003e\n              \u003cp\u003e\n                The following often encountered servers are not pools: \n              \u003c/p\u003e\n                \u003cul\u003e\n                  \u003cli\u003e\u003cdel\u003ekeyserver.pgp.com\u003c/del\u003e\u003c/li\u003e\n                  \u003cli\u003e\u003cdel\u003ekeyserver.ubuntu.com\u003c/del\u003e\u003c/li\u003e\n                  \u003cli\u003e\u003cdel\u003epgp.mit.edu\u003c/del\u003e\u003c/li\u003e\n                \u003c/ul\u003e\n              \u003c/div\u003e\n              \u003chr /\u003e\n              \u003cp\u003e\u003cb\u003eNote:\u003c/b\u003eUsing a single server instead of a pool, makes it easy for a server or network operator to track with whom you communicate. See the \n                \u003ca href=\"https://help.riseup.net/en/security/message-security/openpgp/best-practices\" target=\"_blank\"\u003eOpenPGP Best Practices\u003c/a\u003e guide from riseup.net.\u003c/p\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n\n    \u003c/div\u003e\u003c!--Closing content wrap--\u003e\n    \u003cdiv id=\"footer\"\u003e\n      \u003cdiv class=\"container\"\u003e\n        \u003cdiv style=\"max-width: 20em; float: left;\" \u003e\n          \u003cp class=\"muted credit small\"\u003e\n            \u003cstrong\u003e\u003ca href=\"http://pgpkeysximvxiazm.onion\"\u003e.onion\u003c/a\u003e\u003c/strong\u003e |\n            \u003ca href=\"/stats/\"\u003estats\u003c/a\u003e |\n            \u003ca href=\"https://sks-keyservers.net/status/info/pgpkeys.urown.net\" target=\"_blank\"\u003epool status\u003c/a\u003e |\n            \u003ca href=\"#\" data-toggle=\"modal\" data-target=\"#about\"\u003eabout\u003c/a\u003e |\n            \u003ca href=\"#\" data-toggle=\"modal\" data-target=\"#contact\"\u003econtact\u003c/a\u003e\n          \u003c/p\u003e\n        \u003c/div\u003e\n        \u003cdiv style=\"float:right;\"\u003e\n          \u003cp class=\"muted credit small\"\u003e\n            This is a public service, you are\n            \u003ca href=\"#\" data-toggle=\"modal\" data-target=\"#donate\"\u003ewelcome!\u003c/a\u003e\n          \u003c/p\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cscript src=\"assets/jquery/1.11.2/jquery.min.js\"\u003e\u003c/script\u003e\n    \u003cscript src=\"assets/bootstrap/3.3.2/js/bootstrap.min.js\"\u003e\u003c/script\u003e\n    \u003cscript src=\"assets/jqBootstrapValidation.js/1.3.7/jqBootstrapValidation-1.3.7.min.js\"\u003e\u003c/script\u003e\n  \u003c/body\u003e\n\u003c/html\u003e","pageTitle":"pgpkeys.urown.net - OpenPGP Keyserver","responseHeaders":{"CACHE-CONTROL":"no-cache","CONNECTION":"keep-alive","CONTENT-TYPE":"text/html; charset=utf-8","DATE":"Sun, 24 Jul 2016 05:43:02 GMT","ETAG":"W/\"55bcef1e-31e5\"","EXPIRES":"Sun, 24 Jul 2016 05:43:01 GMT","KEEP-ALIVE":"timeout=600","LAST-MODIFIED":"Sat, 01 Aug 2015 16:09:02 GMT","SERVER":"nginx","VARY":"Accept-Encoding","X-CONTENT-TYPE-OPTIONS":"nosniff","X-FRAME-OPTIONS":"SAMEORIGIN","X-UA-COMPATIBLE":"IE=Edge","X-XSS-PROTECTION":"1; mode=block"},"bitcoinAddress